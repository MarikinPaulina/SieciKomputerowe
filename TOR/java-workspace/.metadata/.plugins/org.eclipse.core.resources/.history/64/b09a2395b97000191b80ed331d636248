import java.util.ArrayList;

public class Test {
	
	ArrayList<Integer> address;
	String message;

	public Test(String m) {
		address = new ArrayList<Integer>();
		for(int ii=0; ii<8; ii++) {
			address.add(ii);
		}
		message=m;
	}
	
	public int getNextAddress() {
		if(address.size() > 0) {
			return address.remove(address.size()-1);
		
	}

}
